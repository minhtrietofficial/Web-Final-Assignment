<div class="home">
    <div class="statusaccount" statusaccount="{{status}}">Tài khoản chưa được xác thực!</div>
    <div class="headerofpage d-flex justify-content-end">
        <div class="content"></div>
        <div class="nameoftransaction">
            
        </div>
        <div class="changepassbutton"><a href="changepassword" style="text-decoration:none">
            <img src="resources/images/icons8-key-96.png" height="50" alt="Changepass" /></a>

        </div>
        <div class="logoutbutton"><a href="logout" style="text-decoration:none">
            <img src="resources/images/icons8-logout-96.png" height="50" alt="Logout" /></a>

        </div>

    </div>
    <div class="sidenav scrollbar-deep-purple bordered-deep-purple thin">
        <div class="logobrandcard">
            <div class="d-flex flex-row logothuonghieu justify-content-center row">
                <div class="logobrand"><a href="home"><img src="resources/images/logo.png" width="90" /></a></div>
                <div class="brandname">
                    BKTPay
                </div>
            </div>
        </div>
        <div class="profilecard">
            <div>
                <img class="avatar" src="resources/images/adminprofile.png" height="80" alt="user" />
            </div>
            <div class="ten">{{fullname}}</div>
            <div class="numberphone">{{numberphone}}</div>

        </div>
        <div class="account" typeaccount="{{typeaccount}}">
            <div class="sodu">Số dư:</div>
            <div class="money">{{money}} VNĐ</div>
        </div>
        <div class="transaction" typeaccount="{{typeaccount}}">
            <div class="transactionname" ><a href="home" style="text-decoration:none">
                    <img class="icon" src="resources/images/icons8-exchange-96.png" height="60" alt="Chuyển tiền" />
                    <div class="nametransaction">Chuyển tiền</div>
                </a>
            </div>
            <div class="transactionname "><a href="home" style="text-decoration:none">
                    <img class="icon" src="resources/images/icons8-money-transfer-96.png" height="60" alt="Nạp tiền" />
                    <div class="nametransaction">Nạp tiền</div>
                </a>
            </div>
            <div class="transactionname "><a href="home" style="text-decoration:none">
                    <img class="icon" src="resources/images/icons8-initiate-money-transfer-96.png" height="60"
                        alt="Rút tiền" />
                    <div class="nametransaction">Rút tiền</div>
                </a>
            </div>
        </div>
        <div class="panel" typeaccount="{{typeaccount}}">
            <div class="panel-group-transaction">
                <div class="panel-group-logo panel-group-logoandname">
                    <img src="resources/images/icons8-phone-96.png" height="60" alt="Nạp tiền điện thoại" />
                </div>
                <div class="panel-group-nametransaction panel-group-logoandname">Nạp tiền điện thoại</div>
                <hr width="100%" color="white" align="center" size="2px">
            </div>
            <div class="panel-group-transaction">
                <div class="panel-group-logo panel-group-logoandname">
                    <img src="resources/images/icons8-medical-history-96.png" height="60" alt="Lịch sử giao dịch" />
                </div>
                <div class="panel-group-nametransaction panel-group-logoandname">Lịch sử giao dịch</div>
                <hr width="100%" color="white" align="center" size="2px">
            </div>
            <div class="panel-group-transaction">
                <div class="panel-group-logo panel-group-logoandname">
                    <img src="resources/images/icons8-mastercard-96.png" height="60" alt="Quản lí thẻ tín dụng" />
                </div>
                <div class="panel-group-nametransaction panel-group-logoandname">Quản lí thẻ tín dụng</div>
                <hr width="100%" color="white" align="center" size="2px">
            </div>
            <div class="panel-group-transaction">
                <div class="panel-group-logo panel-group-logoandname">
                    <img src="resources/images/icons8-delivery-time-96.png" height="60" alt="Giao dịch đang chờ" />
                </div>
                <div class="panel-group-nametransaction panel-group-logoandname">Giao dịch đang chờ</div>
            </div>
        </div>
        <div class="manage" typeaccount="{{typeaccount}}">
            <div class="manage-group-transaction" href="activeaccount">
                <div class="manage-group-logo manage-group-logoandname">
                    <img src="resources/images/icons8-management-96.png" height="60" alt="Nạp tiền điện thoại" />
                </div>
                <a class="manage-group-nametransaction manage-group-logoandname align-center" href="activeaccount" style="text-decoration:none">Kích hoạt tài khoản</a>
                <hr width="100%" color="white" align="center" size="2px">
            </div>
            <div class="manage-group-transaction">
                <div class="manage-group-logo manage-group-logoandname">
                    <img src="resources/images/icons8-denied-96.png" height="60" alt="Nạp tiền điện thoại" />
                </div>
                <div class="manage-group-nametransaction manage-group-logoandname">Tài khoản bị vô hiệu hóa</div>
                <hr width="100%" color="white" align="center" size="2px">
            </div>
            <div class="manage-group-transaction ">
                <div class="manage-group-logo manage-group-logoandname">
                    <img src="resources/images/icons8-unavailable-96.png" height="60" alt="Lịch sử giao dịch" />
                </div>
                <div class="manage-group-nametransaction manage-group-logoandname">Khóa vô thời hạn</div>
                <hr width="100%" color="white" align="center" size="2px">
            </div>
            <div class="manage-group-transaction">
                <div class="manage-group-logo manage-group-logoandname">
                    <img src="resources/images/icons8-transaction-96.png" height="60" alt="Quản lí thẻ tín dụng" />
                </div>
                <div class="manage-group-nametransaction manage-group-logoandname">Phê duyệt chuyển tiền</div>
                <hr width="100%" color="white" align="center" size="2px">
            </div>
            <div class="manage-group-transaction">
                <div class="manage-group-logo manage-group-logoandname">
                    <img src="resources/images/icons8-pay-96.png" height="60" alt="Giao dịch đang chờ" />
                </div>
                <div class="manage-group-nametransaction manage-group-logoandname">Phê duyệt rút tiền</div>
            </div>
        </div>
    </div>
    
    <div class="content">
        

        
    </div>

</div>